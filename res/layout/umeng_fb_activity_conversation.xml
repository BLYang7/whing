<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/navigation_height"
        android:background="@drawable/navigation_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/feedback_navigation_left"
            android:layout_width="@dimen/navigation_logo_dimen"
            android:layout_height="@dimen/navigation_logo_dimen"
            android:layout_marginLeft="@dimen/navigation_logo_left_margin"
             />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/navigation_text_logo_margin"
            android:layout_marginRight="@dimen/new_one_right_margin"
            android:layout_weight="0.79"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/feedback_title"
            android:textColor="@android:color/white"
            android:textSize="@dimen/new_one_text_size" />
    </LinearLayout>
    
    <RelativeLayout 
    android:id="@+id/umeng_fb_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    </RelativeLayout>
    
    
</LinearLayout>


