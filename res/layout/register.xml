<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:orientation="vertical" >
    
     <LinearLayout
	    android:layout_width="fill_parent"
	    android:layout_height="@dimen/navigation_height"
	    android:background="@drawable/navigation_bg"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >

		<Button
		    android:id="@+id/register_navigation_left"
		    android:layout_width="@dimen/navigation_back_width"
            android:layout_height="@dimen/navigation_back_height"
		    android:layout_marginLeft="@dimen/navigation_logo_left_margin"
		    android:background="@drawable/navigation_back" />
	
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.79"
	        android:layout_marginLeft="@dimen/navigation_text_logo_margin"
	        android:layout_marginRight="@dimen/new_one_right_margin"
	        android:gravity="center"
	        android:singleLine="true"
	        android:text="@string/register_title"
	        android:textColor="@android:color/white"
	        android:textSize="@dimen/new_one_text_size" />
	
	</LinearLayout>
	
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginTop="@dimen/register_margin_navigation"
        android:layout_marginBottom="@dimen/register_margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:gravity="bottom"
        >
        <TextView
            android:layout_width="@dimen/text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_view_margin_left"
            android:text="@string/register_user_account"
            android:textSize="@dimen/text_size"
            android:gravity="center"
            >
        </TextView>
        <EditText
            android:id="@+id/register_user_account"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
           
            android:singleLine="true"
        	android:background="@drawable/register_text_bg"
        	android:layout_marginRight="@dimen/text_view_margin_left"
            >
        </EditText>
     </LinearLayout>
     
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginBottom="@dimen/register_margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:gravity="bottom"
        >
        <TextView
            android:layout_width="@dimen/text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_view_margin_left"
            android:text="@string/register_user_mobile"
            android:gravity="center"
            android:textSize="@dimen/text_size"
            >
        </TextView>
        <EditText
            android:id="@+id/register_user_mobile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        	android:background="@drawable/register_text_bg"
        	android:numeric="integer"
            android:singleLine="true"
        	android:layout_marginRight="@dimen/text_view_margin_left"
            >
        </EditText>
     </LinearLayout>
     
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginBottom="@dimen/register_margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:gravity="bottom"
        >
        <TextView
            android:layout_width="@dimen/text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_view_margin_left"
            android:text="@string/register_user_gender"
            android:gravity="center"
            android:textSize="@dimen/text_size"
            >
        </TextView>
        <EditText
            android:id="@+id/register_user_gender"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        	android:background="@drawable/register_text_bg"
            android:singleLine="true"
            android:focusable="false"
            android:clickable="true"
        	android:layout_marginRight="@dimen/text_view_margin_left"
            >
        </EditText>
     </LinearLayout>
	
	
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginBottom="@dimen/register_margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:gravity="bottom"
        >
        <TextView
            android:layout_width="@dimen/text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_view_margin_left"
            android:text="@string/register_user_password"
            android:gravity="center"
            android:textSize="@dimen/text_size"
            >
        </TextView>
        <EditText
            android:id="@+id/register_user_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        	android:background="@drawable/register_text_bg"
        	android:inputType="textPassword"
            android:singleLine="true"
        	android:layout_marginRight="@dimen/text_view_margin_left"
            >
        </EditText>
     </LinearLayout>
     
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginBottom="@dimen/register_margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:gravity="bottom"
        >
        <TextView
            android:layout_width="@dimen/text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_view_margin_left"
            android:gravity="center"
            android:text="@string/register_user_password_again"
            android:textSize="@dimen/text_size"
            >
        </TextView>
        <EditText
            android:id="@+id/register_user_password_again"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:singleLine="true"
        	android:background="@drawable/register_text_bg"
        	android:layout_marginRight="@dimen/text_view_margin_left"
            >
        </EditText>
     </LinearLayout>
     
	
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_left"
        android:layout_marginRight="@dimen/margin_right"
        android:layout_marginTop="@dimen/margin_top"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:paddingTop="@dimen/register_padding_top"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:paddingRight="@dimen/register_padding_right"
        android:gravity="right"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/register_ensure"
            android:layout_width="@dimen/user_new_one_accept_size"
            android:layout_height="@dimen/user_new_one_accept_size"
            android:background="@drawable/radio_button_new_one_normal"
            android:layout_marginTop="@dimen/margin_top"
            android:text="@string/register_ensure"
            android:textSize="@dimen/text_size"
            android:textColor="@android:color/white"
            />
     </LinearLayout>
	
</LinearLayout>
