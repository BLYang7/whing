            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/text_view_margin_left"
            android:text="" >
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/register_margin_left"
        android:layout_marginRight="@dimen/register_margin_right"
        android:layout_marginTop="@dimen/register_margin_bottom"
        android:gravity="bottom"
        android:paddingBottom="@dimen/register_padding_bottom"
        android:paddingTop="@dimen/register_padding_top" >

