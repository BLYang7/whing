            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/text_view_margin_left"
            android:text="" >
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
