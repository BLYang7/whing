            android:text="" >
        </TextView>
    </LinearLayout>

    <Button
        android:id="@+id/inf_show_user_focus"
        android:layout_width="@dimen/user_new_one_accept_size"
        android:layout_height="@dimen/user_new_one_accept_size"
        android:layout_gravity="right"
        android:layout_marginRight="@dimen/my_inf_ensure_button_margin_right"
        android:background="@drawable/radio_button_new_one_normal"
        android:text=""
        android:textColor="@android:color/white"
        android:textSize="@dimen/text_size" />

</LinearLayout>